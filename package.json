    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "dotenv": "^5.0.1",
    "jest": "^22.4.3",
    "nodemon": "^1.17.3",
    "regenerator-runtime": "^0.11.1",
    "rimraf": "^2.6.2",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.3",
    "morgan": "^1.9.0",
    "throw.js": "^3.0.1"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
